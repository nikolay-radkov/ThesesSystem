@model ThesesSystem.Web.ViewModels.Specialties.SpecialtyProfileViewModel
@{
    ViewBag.Title = "Профил на специалност";
}

<h2>@Html.DisplayFor(m => Model.Title)</h2>


<div class="row">
    <div class="col-md-offset-3 col-md-3">
        @Html.LabelFor(m => Model.StudentsNumber)
    </div>
    <div class="col-md-3">
        @Html.DisplayFor(m => Model.StudentsNumber)
    </div>
</div>
<div class="row">
    <div class="col-md-offset-3 col-md-3">
        @Html.LabelFor(m => Model.ThesisCompleteNummber)
    </div>
    <div class="col-md-3">
        @Html.DisplayFor(m => Model.ThesisCompleteNummber)
    </div>
</div>
<div class="row">
    <div class="col-md-offset-3 col-md-3">
        @Html.LabelFor(m => Model.ThesisInProressNumber)
    </div>
    <div class="col-md-3">
        @Html.DisplayFor(m => Model.ThesisInProressNumber)
    </div>
</div>

<div class="row">
    <h2>Факултети</h2>
    @foreach (var item in Model.StudentsList)
    {
        @Html.ActionLink(item.FullName, "Account", "User", new { area = string.Empty, id = item.Id }, new { @class = "thumbnail inline-block" })

    }
</div>

<div class="row">
    <div class="col-lg-offset-3 text-center">
        @Html.Partial("_PaginationPartial", (ThesesSystem.Web.ViewModels.Partials.PaginationViewModel)ViewData["Pagination"])
    </div>
</div>

